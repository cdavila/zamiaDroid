package uni.projecte.dataLayer.RemoteDBManager.objects;

public class RemoteDB {


	private int dbId;
	
	private String dbName;
	private String dbDesc;
	private String dbFilum;
	
	private int order;
	private boolean enabled;
	
	
	public RemoteDB(int dbId, String dbName, String dbDesc, String dbFilum,int order, boolean enabled) {
		
		this.dbId = dbId;
		this.dbName = dbName;
		this.dbDesc = dbDesc;
		this.dbFilum = dbFilum;
		this.order = order;
		this.enabled = enabled;
	}
	
	
		public int getDbId() {
			return dbId;
		}
		public String getDbName() {
			return dbName;
		}
		public String getDbDesc() {
			return dbDesc;
		}
		public String getDbFilum() {
			return dbFilum;
		}
		public int getOrder() {
			return order;
		}
		public boolean isEnabled() {
			return enabled;
		}
		public void setDbId(int dbId) {
			this.dbId = dbId;
		}
		public void setDbName(String dbName) {
			this.dbName = dbName;
		}
		public void setDbDesc(String dbDesc) {
			this.dbDesc = dbDesc;
		}
		public void setDbFilum(String dbFilum) {
			this.dbFilum = dbFilum;
		}
		public void setOrder(int order) {
			this.order = order;
		}
		public void setEnabled(boolean enabled) {
			this.enabled = enabled;
		}
	
	
}