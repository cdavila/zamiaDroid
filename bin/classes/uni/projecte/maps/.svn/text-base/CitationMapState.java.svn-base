package uni.projecte.maps;

public class CitationMapState {
	
	public boolean gpsMode;
	public boolean gpsOn;
	public boolean labelsOn;
	public boolean editModeOn;
	public boolean gridModeOn;
	public boolean myTracksOn;
	public boolean myTracksWorking;
	public boolean myTrackLoaded;
	public boolean compassEnabled;
	public boolean askedGPSUpdate;
	public boolean elevationEnabled;
	public boolean noGPS;
	public boolean movCitationEnabled;

	public CitationMapState(boolean gpsOn, boolean labelsOn,
			boolean editModeOn, boolean gridModeOn, boolean myTracksOn,
			boolean myTracksWorking, boolean myTrackLoaded,
			boolean compassEnabled, boolean askedGPSUpdate,
			boolean elevationEnabled, boolean noGPS,boolean movCitationEnabled) {
		
		this.gpsOn = gpsOn;
		this.labelsOn = labelsOn;
		this.editModeOn = editModeOn;
		this.gridModeOn = gridModeOn;
		this.myTracksOn = myTracksOn;
		this.myTracksWorking = myTracksWorking;
		this.myTrackLoaded = myTrackLoaded;
		this.compassEnabled = compassEnabled;
		this.askedGPSUpdate = askedGPSUpdate;
		this.elevationEnabled = elevationEnabled;
		this.noGPS = noGPS;
		this.movCitationEnabled=movCitationEnabled;
		
	}
}