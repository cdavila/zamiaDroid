<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="200dp"
  android:layout_height="wrap_content"
  android:orientation="vertical"
  android:layout_margin="4dp"
  android:padding="4dp"
  >
	<TextView android:text="@string/projectName" android:id="@+id/TextView01S" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
	<EditText android:hint="@string/projectName"  android:id="@+id/etProjectName" android:layout_width="fill_parent" android:layout_height="wrap_content"></EditText>
	<Button android:text="@string/btExportProject" android:id="@+id/btExportProject" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>

</LinearLayout>
